{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Delve (Docker)",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "host": "127.0.0.1",
      "port": 40000,
      "apiVersion": 2,
      "remotePath": "/service/src",
      "substitutePath": [
        {
          "from": "${workspaceFolder}/src",
          "to": "/service/src"
        }
      ],
      "showLog": true
    }
  ]
}
